<h1>Welcome to delivery!</h1>

<p style= "color:red"><%= notice %></p>

<% if current_user  %>
    <p>Logged as <strong><%= current_user.email %></strong></p>
    <%= button_to "Sign_out", destroy_user_session_path, method: :delete %>
<% else %>
    <%= link_to 'Sign_in', new_user_session_path %>
    or
    <%= link_to 'Sign_up', new_user_registration_path %>
<% end %>